{
  "name": "sceditor",
  "type": "module",
  "version": "2.1.3",
  "description": "A lightweight HTML and BBCode WYSIWYG editor.",
  "homepage": "http://www.sceditor.com/",
  "bugs": "https://github.com/samclarke/SCEditor/issues",
  "license": "MIT",
  "author": "Sam Clarke <sam@samclarke.com>",
  "contributors": [
    {
      "name": "brunoais"
    }
  ],
  "files": [
